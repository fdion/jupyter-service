#!/usr/local/bin/python2.7
# EASY-INSTALL-ENTRY-SCRIPT: 'blaze==0.8.2','console_scripts','blaze-server'
__requires__ = 'blaze==0.8.2'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('blaze==0.8.2', 'console_scripts', 'blaze-server')()
    )
